{
  "trigger": {
    "schedule": {
      "interval": "1m"
    }
  },
  "input": {
    "search": {
      "request": {
        "indices": [
          "<human-logstash-{now/d{YYYY.MM.dd}}>",
          "<human-logstash-{now/d-1d{YYYY.MM.dd}}>",
          "<human-logstash-{now/d-2d{YYYY.MM.dd}}>",
          "<human-logstash-{now/d-3d{YYYY.MM.dd}}>",
          "<human-logstash-{now/d-4d{YYYY.MM.dd}}>",
          "<human-logstash-{now/d-5d{YYYY.MM.dd}}>",
          "<human-logstash-{now/d-6d{YYYY.MM.dd}}>",
          "<human-logstash-{now/d-7d{YYYY.MM.dd}}>",
          "<human-logstash-{now/d-8d{YYYY.MM.dd}}>"
        ],
        "body": {
          "query": {
            "bool": {
              "filter": [
                {
                  "term": {
                    "params.affiliate": "nps.gov"
                  }
                },
                {
                  "terms": {
                    "type": ["search"]
                  }
                },
                {
                  "range": {
                    "@timestamp": {
                      "gte": "{{ctx.trigger.scheduled_time}}||-1w",
                      "lte": "{{ctx.trigger.scheduled_time}}"
                    }
                  }
                }
              ],
              "must_not": [
                {
                  "term": {
                    "useragent.device": "Spider"
                  }
                },
                {
                  "term": {
                    "params.query.raw": ""
                  }
                },
                {
                  "exists": {
                    "field": "modules"
                  }
                },
                {
                  "terms": {
                    "params.query.raw": [
                      "foo",
                      "bar",
                      "another one"
                    ]
                  }
                }
              ]
            }
          },
          "aggs": {
            "agg": {
              "terms": {
                "min_doc_count": 34,
                "size": 10,
                "field": "params.query.raw"
              }
            }
          },
          "size": 0
        }
      }
    }
  },
  "condition": {
    "script": {
      "source": "ctx.payload.aggregations.agg.buckets.size() > 0",
      "lang": "painless"
    }
  },
  "throttle_period": "24h",
  "transform": {
    "script": {
      "source": "ctx.payload.aggregations.agg.buckets.collect(it -> it.key)",
      "lang": "painless"
    }
  },
  "actions": {
    "analytics_alert": {
      "email": {
        "to": "affiliate_manager@fixtures.org",
        "from": "no-reply@support.digitalgov.gov",
        "bcc": "usasearchoutbound@search.gov",
        "subject": "[Search.gov] {{ctx.metadata.alert_name}} (Custom Alert)",
        "body": {
