#container
  background-color: <%= render_affiliate_css_property_value(@css_hash, :content_background_color) %>
#header.managed, #usasearch_footer.managed
  background-color: <%= render_affiliate_css_property_value(@css_hash, :search_button_text_color) %>
  color: <%= render_affiliate_css_property_value(@css_hash, :search_button_background_color) %>
  a, a:visited
    color: <%= render_affiliate_css_property_value(@css_hash, :search_button_background_color) %>
#search_button, #cdr_search_button
  background-color: <%= render_affiliate_css_property_value(@css_hash, :search_button_background_color) %>
  border: 1px solid <%= render_affiliate_css_property_value(@css_hash, :search_button_background_color) %>
  color: <%= render_affiliate_css_property_value(@css_hash, :search_button_text_color) %>
.navigations, .time-filters, .sort-filters
  li div
    border-left-color: <%= render_affiliate_css_property_value(@css_hash, :left_tab_text_color) %>
    color: <%= render_affiliate_css_property_value(@css_hash, :left_tab_text_color) %>
#left_column h3, #left_nav_label, #left_column .facet-name, #left_column .facet .selected
  color: <%= render_affiliate_css_property_value(@css_hash, :left_tab_text_color) %>
.default .affiliate_autocomplete .ui-menu-item
  a, span
    font-family: <%= render_affiliate_css_property_value(@css_hash, :font_family) %>
#search, #search_query
  font-family: <%= render_affiliate_css_property_value(@css_hash, :font_family) %>
#search, #serp-alert
  color: <%= render_affiliate_css_property_value(@css_hash, :description_text_color) %>
  a
    color: <%= render_affiliate_css_property_value(@css_hash, :title_link_color) %>
    &:visited
      color: <%= render_affiliate_css_property_value(@css_hash, :visited_title_link_color) %>
  #left_column
    a:visited, #show_options, #hide_options, .more-facets, .less-facets
      color: <%= render_affiliate_css_property_value(@css_hash, :title_link_color) %>
    .triangle.show-options
      border-top-color: <%= render_affiliate_css_property_value(@css_hash, :title_link_color) %>
    .triangle.hide-options
      border-bottom-color: <%= render_affiliate_css_property_value(@css_hash, :title_link_color) %>
#skiplink, #results .time-filters, #results .sort-filters
  a, a:visited
    color: <%= render_affiliate_css_property_value(@css_hash, :title_link_color) %>
    &.selected
      color: <%= render_affiliate_css_property_value(@css_hash, :left_tab_text_color) %>
#results .current-time-filter, #results .current-sort-filter, #results .clear-filter a
  color: <%= render_affiliate_css_property_value(@css_hash, :left_tab_text_color) %>
  .triangle.show-options
    border-top-color: <%= render_affiliate_css_property_value(@css_hash, :left_tab_text_color) %>
  .triangle.hide-options
    border-bottom-color: <%= render_affiliate_css_property_value(@css_hash, :left_tab_text_color) %>
#results .searchresult
  .agency, .medline, #form_govbox
    h3, .official-tag, .date-label
      color: <%= render_affiliate_css_property_value(@css_hash, :left_tab_text_color) %>
  h3 a, h4 .feed-name, h3.result-url
    color: <%= render_affiliate_css_property_value(@css_hash, :url_link_color) %>
#results
  .govbox
    h3.alt, h3.alt a
      color: <%= render_affiliate_css_property_value(@css_hash, :left_tab_text_color) %>
  .featured-collection h2, .featured-collection h2 a
    color: <%= render_affiliate_css_property_value(@css_hash, :left_tab_text_color) %>
  #form_govbox .check-mark
    background-color: <%= render_affiliate_css_property_value(@css_hash, :search_button_background_color) %>
    border-color: <%= render_affiliate_css_property_value(@css_hash, :search_button_background_color) %>
#results .pagination
  a, a:visited
    color: <%= render_affiliate_css_property_value(@css_hash, :title_link_color) %>
  a:hover, em
    background-color: <%= render_affiliate_css_property_value(@css_hash, :search_button_background_color) %>
    color: <%= render_affiliate_css_property_value(@css_hash, :search_button_text_color) %>
#twitter_govbox, #jobs_govbox
  h3.alt
    color: <%= render_affiliate_css_property_value(@css_hash, :left_tab_text_color) %>
#search .page-not-found
  h3.message
    color: <%= render_affiliate_css_property_value(@css_hash, :left_tab_text_color) %>
#search form.advanced legend
  color: <%= render_affiliate_css_property_value(@css_hash, :description_text_color) %>
#usasearch_footer_container
  background-color: <%= render_affiliate_css_property_value(@css_hash, :page_background_color) %>
  #usasearch_footer
    background-color: <%= render_affiliate_css_property_value(@css_hash, :content_background_color) %>
#usasearch_footer_container #usasearch_footer_button
  color: <%= render_affiliate_css_property_value(@css_hash, :search_button_text_color) %>
  background-color: <%= render_affiliate_css_property_value(@css_hash, :search_button_background_color) %>

