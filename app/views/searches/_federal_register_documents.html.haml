#federal-register-documents
  .content-block-item.content-block-item-header
    %h3.content-heading
      Federal Register documents about #{search.query}
  - search.federal_register_documents.results.first(3).each_with_index do |document, index|
    = render partial: 'federal_register_document',
             locals: { agency: search.affiliate.agency, document: document, position: index + 1 }
  .content-block-item.content-block-item-footer
    .see-more= link_to_federal_register_advanced_search(search)
